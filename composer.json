{
	"name": "baraja-core/selectbox-tree",
	"description": "Simple tree builder to plain selectbox array.",
	"homepage": "https://github.com/baraja-core/selectbox-tree",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.4.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.52",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.9",
		"symplify/easy-coding-standard": "^7.2"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"config": {
		"platform": {
			"php": "7.4.0"
		}
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 6 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
